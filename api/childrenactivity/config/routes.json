{
  "routes": [
    {
      "method": "GET",
      "path": "/childrenactivities",
      "handler": "childrenactivity.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/childrenactivities/count",
      "handler": "childrenactivity.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/childrenactivities/:id",
      "handler": "childrenactivity.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/childrenactivities",
      "handler": "childrenactivity.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/childrenactivities/:id",
      "handler": "childrenactivity.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/childrenactivities/:id",
      "handler": "childrenactivity.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
