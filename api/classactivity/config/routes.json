{
  "routes": [
    {
      "method": "GET",
      "path": "/classactivities",
      "handler": "classactivity.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/classactivities/count",
      "handler": "classactivity.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/classactivities/:id",
      "handler": "classactivity.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/classactivities",
      "handler": "classactivity.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/classactivities/:id",
      "handler": "classactivity.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/classactivities/:id",
      "handler": "classactivity.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
