{
  "routes": [
    {
      "method": "GET",
      "path": "/cleanings",
      "handler": "cleaning.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/cleanings/count",
      "handler": "cleaning.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/cleanings/:id",
      "handler": "cleaning.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/cleanings",
      "handler": "cleaning.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/cleanings/:id",
      "handler": "cleaning.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/cleanings/:id",
      "handler": "cleaning.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
