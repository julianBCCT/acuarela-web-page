{
  "routes": [
    {
      "method": "GET",
      "path": "/daycares",
      "handler": "daycare.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/daycares/count",
      "handler": "daycare.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/daycares/:id",
      "handler": "daycare.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/daycares",
      "handler": "daycare.create",
      "config": {
        "policies": [""]
      }
    },
    {
      "method": "PUT",
      "path": "/daycares/:id",
      "handler": "daycare.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/daycares/:id",
      "handler": "daycare.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
