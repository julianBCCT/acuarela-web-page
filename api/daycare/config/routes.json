{
  "routes": [
    {
      "method": "GET",
      "path": "/daycares/custom-find",
      "handler": "custom-daycare.findWithSubscriptionsAndBilingualUsers",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/daycares",
      "handler": "daycare.find"
    },
    {
      "method": "GET",
      "path": "/daycares/count",
      "handler": "daycare.count"
    },
    {
      "method": "GET",
      "path": "/daycares/:id",
      "handler": "daycare.findOne"
    },
    {
      "method": "POST",
      "path": "/daycares",
      "handler": "daycare.create"
    },
    {
      "method": "PUT",
      "path": "/daycares/:id",
      "handler": "daycare.update"
    },
    {
      "method": "DELETE",
      "path": "/daycares/:id",
      "handler": "daycare.delete"
    }
  ]
}
