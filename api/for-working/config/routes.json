{
  "routes": [
    {
      "method": "GET",
      "path": "/for-workings",
      "handler": "for-working.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/for-workings/count",
      "handler": "for-working.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/for-workings/:id",
      "handler": "for-working.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/for-workings",
      "handler": "for-working.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/for-workings/:id",
      "handler": "for-working.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/for-workings/:id",
      "handler": "for-working.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
