{
  "routes": [
    {
      "method": "GET",
      "path": "/healthinfos",
      "handler": "healthinfo.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/healthinfos/count",
      "handler": "healthinfo.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/healthinfos/:id",
      "handler": "healthinfo.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/healthinfos",
      "handler": "healthinfo.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/healthinfos/:id",
      "handler": "healthinfo.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/healthinfos/:id",
      "handler": "healthinfo.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
