{
  "routes": [
    {
      "method": "GET",
      "path": "/likings",
      "handler": "liking.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/likings/count",
      "handler": "liking.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/likings/:id",
      "handler": "liking.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/likings",
      "handler": "liking.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/likings/:id",
      "handler": "liking.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/likings/:id",
      "handler": "liking.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
