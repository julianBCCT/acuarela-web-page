{
  "routes": [
    {
      "method": "GET",
      "path": "/rols",
      "handler": "rol.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/rols/count",
      "handler": "rol.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/rols/:id",
      "handler": "rol.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/rols",
      "handler": "rol.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/rols/:id",
      "handler": "rol.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/rols/:id",
      "handler": "rol.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
