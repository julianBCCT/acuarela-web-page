{
  "routes": [
    {
      "method": "GET",
      "path": "/supporttickets",
      "handler": "supportticket.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/supporttickets/count",
      "handler": "supportticket.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/supporttickets/:id",
      "handler": "supportticket.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/supporttickets",
      "handler": "supportticket.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/supporttickets/:id",
      "handler": "supportticket.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/supporttickets/:id",
      "handler": "supportticket.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
