{
  "routes": [
    {
      "method": "GET",
      "path": "/visits",
      "handler": "visit.find",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/visits/count",
      "handler": "visit.count",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/visits/:id",
      "handler": "visit.findOne",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/visits",
      "handler": "visit.create",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/visits/:id",
      "handler": "visit.update",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/visits/:id",
      "handler": "visit.delete",
      "config": {
        "policies": ["global::is-authenticated"]
      }
    }
  ]
}
